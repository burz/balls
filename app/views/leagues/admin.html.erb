<div id="admin_page">
  <center>
  <h1>Admin Page For: <%= @league.name %></h1>
  <div class="row">
    <div class="col-md-6">
      <div class="panel panel-success">
        <div class="panel-heading">
          <h2>Seasons</h2>
        </div>
        <div class="panel-body">
          <%= render 'seasons/index' %>
        </div>
      </div>
    </div>
    <div class="col-md-6">
      <div class="panel panel-danger">
        <div class="panel-heading">
          <h2>Members</h2>
        </div>
        <div class="panel-body">
          <%= render template: 'leagues/members' %>
        </div>
      </div>
    </div>
  </div>
  </center>
</div>
